<!DOCTYPE html>
<html>
<head>
  <title>Whatswrongwithpresentations!</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>
	<div class="container">
		<div class="header">
			<%= render 'layouts/header'%>
		</div>
		<div class="main">
			<% flash.each do |key, value| %>
				<div class="flash <%= key %>"><%= value %></div>
			<% end %>
			<%= yield %>
		</div>
		<div class="footer">
			<%= render 'layouts/footer' %>
		</div>
	</div>
</body>
</html>
